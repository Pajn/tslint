// ensure that Bar and Baz do not get conflated
interface Bar {
    x(): void;
}

interface Baz {
    y: number;
}

interface BarBaz {
    x(): void;
    y: number;
    ~~~~~~~~~~ [0]
}

class Foo {
    x(): void {}
    y: number;
    ~~~~~~~~~~ [0]
}

interface IArrowMethods {
    a: number;
    b(): void;
    c: () => void;
}

class ArrowMethods {
    public a = 5;
    public b = "foo";
    public c() {}
    public d = () => {};
    public e = function() {};
}

[0]: Declaration of public instance member variable not allowed to appear after declaration of public instance member function
